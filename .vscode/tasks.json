{
    "version": "2.0.0",
    "tasks": [
      {
        "label": "build-current-file",
        "type": "shell",
        "command": "gcc",
        "args": ["-g", "${file}", "-o", "${fileDirname}/bin/${fileBasenameNoExtension}"],
        "group": "build"
      },
      {
        "label": "build-dev",
        "type": "shell",
        "command": "./command.sh",
        "args": ["debug"],
        "group": "build"
      }
    ]
  }
  